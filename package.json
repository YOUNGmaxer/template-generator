{
  "name": "@azeryang/create",
  "version": "0.0.1",
  "description": "minimalist template generator",
  "bin": {
    "@azeryang/create": "./bin/index.js"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register ./src/index.ts",
    "debug": "cross-env TS_NODE_BASEURL=./out node -r tsconfig-paths/register ./out/index.js",
    "lint": "eslint src/**/*",
    "build": "esbuild ./src/index.ts --bundle --platform=node --outfile=./bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YOUNGmaxer/template-generator.git"
  },
  "author": "azeryang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/YOUNGmaxer/template-generator/issues"
  },
  "homepage": "https://github.com/YOUNGmaxer/template-generator#readme",
  "devDependencies": {
    "@jest/globals": "^29.1.2",
    "@swc/core": "^1.3.7",
    "@swc/jest": "^0.2.22",
    "@types/node": "^18.11.18",
    "@types/prompts": "^2.4.1",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.2",
    "eslint": "^8.32.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "jest": "^29.1.2",
    "prompts": "^2.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  }
}
